import pandas as pd

data = {
    "Name": ["Alice", "Bob", "Charlie", "David"],
    "Age": [25, None, 30, 28],
    "Salary": [50000, 60000, None, 70000]
}

df = pd.DataFrame(data)
print(df)

df["Age"].fillna(df["Age"].mean(), inplace=True)
df["Salary"].fillna(df["Salary"].median(), inplace=True)

print(df)
print(df.describe())
